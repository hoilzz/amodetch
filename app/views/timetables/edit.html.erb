

<div class = "timetable_new_container">
	<div class = "add_form_box">
      <h1>시간표 만들기</h1>
      <br><br>

		<%= form_for @timetable do |f|%>
			<div class = "form-group">
	            <%= f.label :'시간표 이름' %> 
	            <%= f.text_field :name, class: 'form-control', :placeholder => "시간표 이름" %>
            </div>

            <div class = "form-group">
	            <%= f.label :'시간표 학기' %> 
	            <select class="form-control">
	              <option>2016년 1학기</option>
	              <option>2015년 2학기</option>
	            </select>
          	</div>
        <%= f.submit "만들기 !", class: "btn btn-primary btn_attr" %>

<% end %>

